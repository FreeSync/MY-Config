neighbor	2.0 bin
neigh_modify	 delay 0 every 1 check yes
timestep     ${dt}
timestep     0.001

# 1st equilibration run
velocity	all create $t 345352 mom yes rot yes dist gaussian
velocity	all create 300 345352 mom yes rot yes dist gaussian

fix		1 all nvt temp $t $t 10 drag 0.2
fix		1 all nvt temp 300 $t 10 drag 0.2
fix		1 all nvt temp 300 300 10 drag 0.2
thermo	 1000
run		400000
